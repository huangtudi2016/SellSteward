<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black" >

    <LinearLayout
        android:id="@+id/ll_wares"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/wacai_bg"
        android:orientation="vertical" >

        <include layout="@layout/title_bar" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1.0"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="240dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/camera_btn" >

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="@dimen/size6"
                    android:scaleType="centerCrop" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_margin="5dip"
                android:layout_marginTop="20dp"
                android:layout_weight="1.0"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tally_click"
                    android:paddingLeft="@dimen/size10" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="@dimen/size5"
                        android:singleLine="true"
                        android:text="@string/txtName"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF2" />

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.0"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/tvName"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:gravity="right"
                            android:singleLine="true"
                            android:text="韩版外套"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtSizeF3" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1.0px"
                    android:background="@color/divider" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tally_click"
                    android:paddingLeft="@dimen/size10" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="@dimen/size5"
                        android:singleLine="true"
                        android:text="@string/txtType"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF2" />

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.0"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@id/tvType"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:gravity="right|center"
                            android:singleLine="true"
                            android:text="外套"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtSizeF3" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1.0px"
                    android:background="@color/divider" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tally_click"
                    android:paddingLeft="@dimen/size10" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="@dimen/size5"
                        android:singleLine="true"
                        android:text="@string/txtMoneyOut"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF2" />

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.0"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/tvMoneyOut"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:gravity="right|center"
                            android:singleLine="true"
                            android:text="88.00"
                            android:textColor="@color/red"
                            android:textSize="@dimen/txtSizeF3"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1.0px"
                    android:background="@color/divider" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tally_click"
                    android:paddingLeft="@dimen/size10" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="@dimen/size5"
                        android:singleLine="true"
                        android:text="@string/txtCodeSimple"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF2" />

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/tvCode"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right|center"
                            android:singleLine="true"
                            android:text="12345678"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtSizeF3" />
                    </LinearLayout>
                </LinearLayout>
<View
                    android:layout_width="fill_parent"
                    android:layout_height="1.0px"
                    android:background="@color/divider" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tally_click"
                    android:paddingLeft="@dimen/size10" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="@dimen/size5"
                        android:singleLine="true"
                        android:text="@string/txtAmount"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF2" />

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/tvAmount"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right|center"
                            android:singleLine="true"
                            android:text="1"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtSizeF3" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1.0px"
                    android:background="@color/divider" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/listHeight1"
                    android:background="@drawable/bg_tally_click"
                    android:orientation="horizontal"
                    android:layout_gravity="bottom"
                    android:paddingLeft="@dimen/bluePgPaddingLR2"
                    android:paddingRight="@dimen/bluePgPaddingLR2" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_marginRight="@dimen/size5"
                        android:gravity="center_vertical"
                        android:singleLine="true"
                        android:text="出单数:"
                        android:textColor="@color/lightBlue"
                        android:textSize="@dimen/txtSizeF3" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvAmountDown"
                            style="@style/whiteDownShadow"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:singleLine="true"
                            android:text=" -"
                            android:textColor="@color/lightBlue"
                            android:textSize="@dimen/txtSizeF6" />

                        <TextView
                            android:id="@+id/tvAmountOut"
                            style="@style/whiteDownShadow"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="2.0"
                            android:gravity="center"
                            android:singleLine="true"
                            android:text="1"
                            android:textColor="@color/red"
                            android:textSize="@dimen/txtSizeF4" />

                        <TextView
                            android:id="@+id/tvAmountAdd"
                            style="@style/whiteDownShadow"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:singleLine="true"
                            android:text="+"
                            android:textColor="@color/lightBlue"
                            android:textSize="@dimen/txtSizeF5" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <include layout="@layout/bottom_back" />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_gallery3d"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/black"
        android:visibility="gone" >

        <com.fada.sellsteward.gallery_3d.MyGallery
            android:id="@+id/myGallery"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />

        <RelativeLayout
            android:id="@+id/LinBar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" >

            <ProgressBar
                style="?android:attr/progressBarStyleLarge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RLzoomView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/black"
            android:visibility="gone" >

            <com.fada.sellsteward.gallery_3d.ImageZoomView
                android:id="@+id/zoomView"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" />

            <FrameLayout
                android:id="@+id/backGallery"
                android:layout_width="fill_parent"
                android:layout_height="55dip"
                android:layout_alignParentBottom="true"
                android:background="@drawable/bg_camera_pattern" >

                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="15dip"
                    android:layout_marginLeft="140dip"
                    android:layout_marginRight="130dip"
                    android:layout_marginTop="15dip"
                    android:src="@drawable/ic_menu_cancel_holo_light" />
            </FrameLayout>
        </RelativeLayout>
    </FrameLayout>

</FrameLayout>